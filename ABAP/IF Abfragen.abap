IF A EQ B.
    WRITE: 'A ist gleich B'.
ENDIF.

IF A GT B.
    WRITE: 'A ist größer als B'.
ENDIF.

IF A LT B.
    WRITE: 'A ist kleiner als B'.
ENDIF.

IF A NE B.
    WRITE: 'A ist nicht gleich B'.
ENDIF.

IF A GE B.
    WRITE: 'A ist größer oder gleich B'.
ENDIF.

IF A LE B.
    WRITE: 'A ist kleiner oder gleich B'.
ENDIF.

IF A BETWEEN B AND C.
    WRITE: 'A liegt zwischen B und C'.
ENDIF.

IF A IS INITIAL.
    WRITE: 'A ist nicht initialisiert oder leer'.
ENDIF.
